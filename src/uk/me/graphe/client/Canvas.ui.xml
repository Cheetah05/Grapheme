<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:uk.me.graphe.client" 
	xmlns:w="urn:import:com.google.gwt.user.client.ui" >
	<ui:style>
		.select, .move, .zoom, .pan {
			width: 800px;
			height: 800px;
		}
		
		.select {
			cursor: pointer;
		}
		
		.move {
			cursor: move;
		}
		
		.pan {
			cursor: crosshair;
		}
		
		.zoom {
			cursor: crosshair;
		}
	</ui:style>
	<g:CanvasWrapper styleName='{style.select}' ui:field="canvasPanel">
	</g:CanvasWrapper>
</ui:UiBinder>