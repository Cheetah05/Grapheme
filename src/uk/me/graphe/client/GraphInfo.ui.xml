<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	
		.lblTitle {
			color: #111111;
			padding-top: 15px;
			text-shadow: 0px 2px 2px #cccccc;
		}
		
		.lblUsers {
			padding-top: 2px;
			font-weight: normal;
			color: #bbbbbb;
			font-size: 12px;
		}
		
	</ui:style>
	<g:HorizontalPanel>
		<g:Label styleName='{style.lblTitle}' ui:field='lblTitle'></g:Label>
		<g:Label styleName='{style.lblUsers}' ui:field='lblUsers'></g:Label>
	</g:HorizontalPanel>
</ui:UiBinder>