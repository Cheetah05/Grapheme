<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.button {
			width: 150px;
		}
		
		.stack {
			background: #BBDAF0;
			border: 1px solid #587780;
		}
		
		.verticalPanel {
			height: 100px;
			border-top: 1px solid #99BDD1;
			border-left: 1px solid #99BDD1;
			border-right: 1px solid #99BDD1;
		}
		
		.header {
			border-top: 1px solid #99BDD1;
			border-left: 1px solid #99BDD1;
			border-right: 1px solid #99BDD1;
			font-weight: bold;
		}
		
		.header2 {
			border: 1px solid #99BDD1;
			font-weight: bold;
		}
	</ui:style>
	<g:HorizontalPanel styleName='{style.verticalPanel}' ui:field="tools">
				<g:Button styleName='{style.button}' ui:field="button1" >Add Vertex</g:Button>
				<g:Button styleName='{style.button}' ui:field="button2" >Add Edge</g:Button>
				<g:Button styleName='{style.button}' ui:field="button3" >Remove Vertex</g:Button>
				<g:Button styleName='{style.button}' ui:field="button4" >Remove Edge</g:Button>
		
				<g:HorizontalPanel ui:field="options"/>
	</g:HorizontalPanel>

</ui:UiBinder>